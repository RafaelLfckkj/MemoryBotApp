{
  "expo": {
    "name": "MemoryBotApp",
    "slug": "MemoryBotApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/memorybot.png",
    "scheme": "memorybotapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/memorybot.png",
      "resizeMode": "contain",
      "backgroundColor": "#36D9C8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rafaellfckkj.MemoryBotApp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#36D9C8",
        "foregroundImage": "./assets/memorybot.png"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "package": "com.rafaellfckkj.MemoryBotApp"
    },
    "web": {
      "favicon": "./assets/memorybot.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "66e9ca93-618b-4c7b-8a9b-cdbd24d7d075"
      }
    }
  }
}